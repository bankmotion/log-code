# Database Configuration
DATABASE_BRAZZERS=brazzers
DATABASE_AZNUDE=aznude
DATABASE_AZNUDEMEN=aznudemen
DATABASE_AZFANS=azfans

# MySQL Connection
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_PORT=3306

# S3/Wasabi Configuration
WASABI_ENDPOINT=https://s3.us-east-2.wasabisys.com
AWS_PROFILE=wasabi
AWS_REGION=us-east-2

# Directory Paths
MAIN_DIR_AZNUDE=/var/www/html/aznbaby
MAIN_DIR_AZNUDEMEN=/var/www/html/aznbaby/aznudemen
MAIN_DIR_AZFANS=/var/www/html/aznbaby/azfans
CLOUDFLARE_LOG_DIR=/var/www/cloudflarelog

# GS/Cloud Storage Paths
GC_WOMEN_HTML=gs://aznude-html
GC_MEN_HTML=gs://azmen-html
GC_FANS_HTML=gs://azfans-html

# SSH/Server Commands
LEASEWEB_SERVER_SSH=ssh user@server

# Rclone Configuration
RCLONE_ENABLED=false
RCLONE_REMOTE=r2:" > .env.example